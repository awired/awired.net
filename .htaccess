<IfModule mod_rewrite.c>
RewriteEngine On

RewriteCond %{HTTP_HOST} ^www.awired.net$
RewriteRule ^(.*)   http://awired.net/$1  [QSA,L,R=301]

RewriteCond %{HTTP_HOST} ^awired.fr$
RewriteRule ^(.*)   http://awired.net/$1  [QSA,L,R=301]

RewriteCond %{HTTP_HOST} ^www.awired.fr$
RewriteRule ^(.*)   http://awired.net/$1  [QSA,L,R=301]

</IfModule>

